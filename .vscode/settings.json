{
  "workbench.iconTheme": "vscode-icons",
  "javascript.validate.enable": false,
  "flow.useNPMPackagedFlow": true,
  "terminal.integrated.shell.windows":
    "C:\\WINDOWS\\Sysnative\\WindowsPowerShell\\v1.0\\powershell.exe",
  "search.useRipgrep": true,
  "html.format.wrapAttributes": "force-aligned",
  "htmlhint.options": {
    "tagname-lowercase": true,
    "attr-lowercase": true,
    "attr-value-double-quotes": true,
    "doctype-first": false,
    "tag-pair": true,
    "spec-char-escape": true,
    "id-unique": true,
    "src-not-empty": true,
    "attr-no-duplication": true,
    "title-require": true
  },
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "typescript.tsdk": "./node_modules/typescript/lib",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "wwwroot": true,
    "build": true,
    "Scripts": true,
    "node_modules": true,
    "obj": true,
    "bin": true
  },
  "less.lint.compatibleVendorPrefixes": "error",
  "less.lint.vendorPrefix": "error",
  "less.lint.duplicateProperties": "warning",
  "less.lint.universalSelector": "warning",
  "less.lint.zeroUnits": "warning",
  "less.lint.fontFaceProperties": "error",
  "less.lint.ieHack": "warning",
  "less.lint.important": "warning",
  "tslint.autoFixOnSave": true,
  "eslint.autoFixOnSave": true,
  "window.zoomLevel": 0,
  "javascript.referencesCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.useCodeSnippetsOnMethodSuggest": true,
  "tslint.ignoreDefinitionFiles": false
}
